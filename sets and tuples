s=(1,"likith",0.1,)
r=(1,2,3,4,1,1,1)
print(s+r)
print(len(s+r))
print(type(s))
print(min(r))
print(max(r))
c=r.count(1)
print(c)
print(s.index("likith"))
l=[1,22,3,4,5]
t=tuple(l)
print(type(t))
l=list(r)
print(type(l))
print(r*5)
print("")

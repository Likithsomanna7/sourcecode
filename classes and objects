class Python:
    version = input('Enter the version:')
    ide = input('enter ide:')
    def display(self):
        print("\n the py speces:", self.version, self.ide)
class Java:
    version = input('Enter the version:')
    ide = input('enter ide:')
    def display(self):
        print("\n the jv speces:", self.version, self.ide)


P = Python()
P.display()
J = Java()
J.display()


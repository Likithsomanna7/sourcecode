class car():
    def __init__(self,a,b):
        self.a =a
        self.b=b
    def millage(self):
        pass
    def speed(self):
        pass
class Lambo(car):
    def millage(self):
        print("millage of Lambo is 5KMH",self.a)
    def speed(self):
        print("speed is Lambo 400KMH",self.b)

a=int(input("enter millage : "))
b=int(input("enter speed :"))
c=int(input("enter millage : "))
d=int(input("enter speed :"))
k=Lambo(a,b)
k.millage()
k.speed()
l=Lambo(c,d)
l.speed()
l.millage()

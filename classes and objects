class pappa():
    t=10
    def __init__(self,a,b):
        self.a=a
        self.b=b

    def function1(self):
        print("pull strict")
        print(self.a, self.b)
        pass
class kunni():
    def __init__(self, a, b):
        self.a = a
        self.b = b
    def function1(self):
         print(self.a,self.b)
         print("pull gowji")

a=input("enter : ")
b=input("enter : ")
k=kunni(a,b)
k.function1()


